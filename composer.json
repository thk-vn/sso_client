{
    "name": "thk-hd/sso-client",
    "description": "Package Laravel xác thực OAuth2",
    "type": "library",
    "version": "2.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Wake",
            "email": "n_thanhthuc@thk-hd.vn"
        }
    ],
    "autoload": {
        "psr-4": {
            "THKHD\\SsoClient\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "THKHD\\SsoClient\\SSOClientServiceProvider"
            ],
            "aliases": {
                "SSOClient": "THKHD\\SsoClient\\Facades\\SSOClient"
            }
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/http": "^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "prefer-stable": true
}
